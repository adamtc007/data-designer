{
  "metadata": {
    "version": "1.0",
    "description": "Data Dictionary for Rule Attributes",
    "created": "2025-10-06",
    "author": "Data Designer"
  },
  "attributes": [
    {
      "name": "status",
      "display_name": "Status",
      "data_type": "String",
      "description": "Current status of the record",
      "constraints": {
        "allowed_values": ["active", "inactive", "pending", "archived"],
        "required": true,
        "max_length": 20
      },
      "source": {
        "system": "CRM",
        "field": "record_status",
        "table": "customers"
      },
      "tags": ["core", "business"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "country",
      "display_name": "Country Code",
      "data_type": "String",
      "description": "ISO country code",
      "constraints": {
        "pattern": "^[A-Z]{2}$",
        "required": true,
        "max_length": 2
      },
      "source": {
        "system": "Address Service",
        "field": "country_code",
        "table": "addresses"
      },
      "tags": ["location", "regulatory"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "level",
      "display_name": "Customer Level",
      "data_type": "Number",
      "description": "Customer tier level (1-10)",
      "constraints": {
        "min_value": 1,
        "max_value": 10,
        "required": false
      },
      "source": {
        "system": "CRM",
        "field": "customer_tier",
        "table": "customer_profiles"
      },
      "tags": ["business", "tier"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "name",
      "display_name": "Customer Name",
      "data_type": "String",
      "description": "Full customer name",
      "constraints": {
        "required": true,
        "max_length": 100,
        "min_length": 2
      },
      "source": {
        "system": "CRM",
        "field": "full_name",
        "table": "customers"
      },
      "tags": ["identity", "personal"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "user_id",
      "display_name": "User ID",
      "data_type": "String",
      "description": "Unique user identifier",
      "constraints": {
        "pattern": "^[A-Z]{3}\\d{5}$",
        "required": true,
        "max_length": 8
      },
      "source": {
        "system": "Identity",
        "field": "user_id",
        "table": "users"
      },
      "tags": ["identity", "unique"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "type",
      "display_name": "Record Type",
      "data_type": "String",
      "description": "Type classification of the record",
      "constraints": {
        "allowed_values": ["user", "admin", "guest", "system"],
        "required": true,
        "max_length": 10
      },
      "source": {
        "system": "Core",
        "field": "record_type",
        "table": "records"
      },
      "tags": ["classification", "core"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "active",
      "display_name": "Active Flag",
      "data_type": "Boolean",
      "description": "Whether the record is currently active",
      "constraints": {
        "required": true
      },
      "source": {
        "system": "Core",
        "field": "is_active",
        "table": "records"
      },
      "tags": ["status", "core"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "role",
      "display_name": "User Role",
      "data_type": "String",
      "description": "Role assignment for the user",
      "constraints": {
        "allowed_values": ["Admin", "User", "Manager", "Guest"],
        "required": true,
        "max_length": 20
      },
      "source": {
        "system": "Identity",
        "field": "role_name",
        "table": "user_roles"
      },
      "tags": ["security", "authorization"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "region",
      "display_name": "Geographic Region",
      "data_type": "String",
      "description": "Geographic region code",
      "constraints": {
        "allowed_values": ["NA", "EU", "APAC", "LATAM", "MEA"],
        "required": false,
        "max_length": 5
      },
      "source": {
        "system": "Geography",
        "field": "region_code",
        "table": "regions"
      },
      "tags": ["location", "business"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "country_code",
      "display_name": "Country Code",
      "data_type": "String",
      "description": "Two-letter country identifier",
      "constraints": {
        "pattern": "^[A-Z]{2}$",
        "required": true,
        "max_length": 2
      },
      "source": {
        "system": "Geography",
        "field": "iso_country",
        "table": "countries"
      },
      "tags": ["location", "standard"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "enabled",
      "display_name": "Enabled Status",
      "data_type": "Boolean",
      "description": "Whether the feature is enabled",
      "constraints": {
        "required": true
      },
      "source": {
        "system": "Config",
        "field": "enabled",
        "table": "feature_flags"
      },
      "tags": ["config", "feature"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "price",
      "display_name": "Unit Price",
      "data_type": "Number",
      "description": "Price per unit in base currency",
      "constraints": {
        "min_value": 0.0,
        "decimal_places": 2,
        "required": true
      },
      "source": {
        "system": "Pricing",
        "field": "unit_price",
        "table": "products"
      },
      "tags": ["financial", "pricing"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "quantity",
      "display_name": "Quantity",
      "data_type": "Number",
      "description": "Number of items or units",
      "constraints": {
        "min_value": 0,
        "integer_only": true,
        "required": true
      },
      "source": {
        "system": "Inventory",
        "field": "qty",
        "table": "inventory"
      },
      "tags": ["inventory", "numeric"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "tax",
      "display_name": "Tax Amount",
      "data_type": "Number",
      "description": "Tax amount in base currency",
      "constraints": {
        "min_value": 0.0,
        "decimal_places": 2,
        "required": false
      },
      "source": {
        "system": "Taxation",
        "field": "tax_amount",
        "table": "tax_calculations"
      },
      "tags": ["financial", "tax"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "base_rate",
      "display_name": "Base Rate",
      "data_type": "Number",
      "description": "Base rate as decimal percentage",
      "constraints": {
        "min_value": 0.0,
        "max_value": 1.0,
        "decimal_places": 4,
        "required": true
      },
      "source": {
        "system": "Pricing",
        "field": "base_rate",
        "table": "rate_tables"
      },
      "tags": ["financial", "rate"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    },
    {
      "name": "tier",
      "display_name": "Service Tier",
      "data_type": "String",
      "description": "Service tier classification",
      "constraints": {
        "allowed_values": ["basic", "standard", "premium", "enterprise"],
        "required": true,
        "max_length": 15
      },
      "source": {
        "system": "Billing",
        "field": "service_tier",
        "table": "subscriptions"
      },
      "tags": ["business", "tier"],
      "created_date": "2025-10-06",
      "last_modified": "2025-10-06"
    }
  ],
